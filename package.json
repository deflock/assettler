{
  "name": "assettler-monorepo",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=8.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap --concurrency 1",
    "clean": "lerna clean && lerna run clean",
    "compile": "lerna run compile",
    "recompile": "lerna run recompile",
    "version": "lerna version --npm-client=npm",
    "release": "lerna publish --npm-client=npm",
    "outdated": "lerna exec --no-bail --stream -- yarn outdated",
    "pretest": "lerna run pretest",
    "test": "jest",
    "cover": "jest --coverage",
    "watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "cross-env": "^5.2.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "lerna": "^3.8.0",
    "rimraf": "^2.6.3"
  }
}
